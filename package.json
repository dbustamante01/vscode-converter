{
    "name": "converter",
    "displayName": "Converter",
    "description": "Conversion helper for Hex to Ascii and Ascii to Hex",
    "version": "0.0.1",
    "publisher": "cahitbeyaz",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.hex2ascii","onCommand:extension.ascii2hex"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.hex2ascii",
            "title": "Convert: Hex To Ascii, h2a"
        },{
            "command": "extension.ascii2hex",
            "title": "Convert: Ascii To Hex, a2h"
        }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}